		"jsonwebtoken": "^9.0.0",
		"jwt-decode": "^3.1.2",
		"nestjs-cls": "^3.3.1",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0"
	},
